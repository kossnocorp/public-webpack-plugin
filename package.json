  },
  "homepage": "https://github.com/kossnocorp/static-files-webpack-plugin",
  "devDependencies": {
    "espower-loader": "^1.0.0",
    "file-loader": "^1.1.11",
    "mocha": "^5.2.0",
    "power-assert": "^1.1.0",
    "rimraf": "^2.4.3",
    "standard": "^12.0.1",
    "static-file-loader": "^0.6.0",
    "webpack": "^4.5.0"
